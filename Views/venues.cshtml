<h1>Band Tracker</h1>
<h1>Here are all of the venues</h1>
@if(@Model.Count != 0)
{
  @foreach(var venue in Model)
  {
    <li><a href="/venues/@venue.GetId()">@venue.GetName()</a></li>
  }
}
else
{
  <p>There is currently no venues.</p>
}
<form action="/venues/clear" method="post">
  <button type="submit" name="button">clear all venues</button>
</form>
<p><a href="/venues/new">Add a new venue</a></p>
<p><a href="/">Return Home</a></p>
