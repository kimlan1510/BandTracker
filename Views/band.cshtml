<h1>Band Tracker</h1>
<h1>Band: @Model["band"].GetName()</h1>
@if(@Model["venues"].Count != 0)
{
  <h2>Venue(s):</h2>
  @foreach(var venue in Model["venues"])
  {
    <li><a href="/venues/@venue.GetId()">@venue.GetName()</a></li>
  }
}
else
{
  <p>
    This band currently does not perform anywhere!
  </p>
}
<p><a href="/bands/@Model["band"].GetId()/venue">Add where this band will perform</a></p>
<p><a href="/">Return Home</a></p>
